    "version": "0.2.0",
    "configurations": [
        {
            "name": "cbs",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/pastar",
            "args": ["-m", "random-32-32-20.map", "-a", "random-32-32-20-random-1.scen", "-o", "test.csv", "--outputPaths=paths.txt", 
                "-k", "80", "-t", "60", "-s", "2"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Cmake Debug",
            //"miDebuggerPath": "/usr/bin/gdb"
        }

    ]
}